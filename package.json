{
  "name": "textlint-rule-eslint",
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/textlint-rule-eslint.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/azu/textlint-rule-eslint",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azu/textlint-rule-eslint/issues"
  },
  "files": [
    "src/",
    "lib/"
  ],
  "version": "1.1.2",
  "description": "textlint rule integrate with ESLint.",
  "main": "lib/textlint-rule-eslint.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/",
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build"
  },
  "keywords": [
    "eslint",
    "textlint"
  ],
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-jsdoc-to-assert": "^2.0.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "mocha": "^2.5.3",
    "power-assert": "^1.4.1",
    "textlint-tester": "^2.0.0"
  },
  "dependencies": {
    "eslint": "^3.0.0"
  }
}
